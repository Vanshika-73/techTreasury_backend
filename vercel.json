{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/uploads/(.*)", "dest": "/public/uploads/$1" },
        { "src": "/api/(.*)", "dest": "/api/index.js" },
        { "src": "/(.*)", "dest": "/api/index.js" }
    ]
}
